<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

	<changeSet author="unieventos" id="1740711069356">
		<preConditions>
			<not>
				<tableExists tableName="evento_categoria" />
			</not>
		</preConditions>

		<createTable tableName="evento_categoria">
			<column name="id_evento" type="VARCHAR(36)">
				<constraints primaryKey="true" nullable="false"
					foreignKeyName="fk_evento_categoria_evento"
					referencedTableName="evento" referencedColumnNames="id" />
			</column>
			<column name="id_categoria" type="VARCHAR(36)">
				<constraints primaryKey="true" nullable="false"
					foreignKeyName="fk_evento_categoria_categoria"
					referencedTableName="categoria" referencedColumnNames="id" />
			</column>
		</createTable>
	</changeSet>

</databaseChangeLog>