<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
	
	<changeSet author="unieventos" id="1740710923957" >
		<preConditions>
			<not>
				<tableExists tableName="fotos"/>
			</not>
		</preConditions>
	
		<createTable tableName="fotos">
		
            <column name="id" type="VARCHAR(36)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            
            <column name="id_evento" type="VARCHAR(36)">
                <constraints nullable="false" foreignKeyName="fk_foto_evento"
                             referencedTableName="evento" referencedColumnNames="id"/>
            </column>
            
            <column name="path" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            
        </createTable>
	</changeSet>
	
</databaseChangeLog>