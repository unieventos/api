<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

	<changeSet author="unieventos" id="1740711356269">
		<preConditions>
			<not>
				<tableExists tableName="usuario_evento_permissao" />
			</not>
		</preConditions>
		
		<createTable tableName="usuario_evento_permissao">
            <column name="id" type="VARCHAR(36)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            
            <column name="id_evento" type="VARCHAR(36)">
                <constraints nullable="false" foreignKeyName="fk_usuario_evento_permissao_evento"
                             referencedTableName="evento" referencedColumnNames="id"/>
            </column>
            
            <column name="id_usuario" type="VARCHAR(36)">
                <constraints nullable="false" foreignKeyName="fk_usuario_evento_permissao_usuario"
                             referencedTableName="usuario" referencedColumnNames="id"/>
            </column>
        </createTable>
        
	</changeSet>

</databaseChangeLog>